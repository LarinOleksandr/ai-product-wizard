# Local AI services (Docker + Ollama)
EMBEDDING_API=http://host.docker.internal:8001
ORCHESTRATOR_API=http://host.docker.internal:8002
OLLAMA_API=http://host.docker.internal:11434

# Edge Functions calling Supabase (do NOT use SUPABASE_* names; runtime skips them)
EDGE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
EDGE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_PUBLIC_KEY

# Exports to Supabase Storage (service role for uploads)
EXPORT_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
EXPORT_SUPABASE_SERVICE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
EXPORT_BUCKET=exports
